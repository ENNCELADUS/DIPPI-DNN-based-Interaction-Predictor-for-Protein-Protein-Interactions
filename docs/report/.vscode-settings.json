{
    "markdown-pdf.styles": ["custom-pdf-style.css"],
    "markdown-pdf.includeDefaultStyles": false,
    "markdown-pdf.displayHeaderFooter": false,
    "markdown-pdf.headerTemplate": "",
    "markdown-pdf.footerTemplate": "",
    "markdown-pdf.margin": {
        "top": "0.75in",
        "bottom": "0.75in",
        "left": "0.75in",
        "right": "0.75in"
    },
    "markdown-pdf.format": "A4",
    "markdown-pdf.orientation": "portrait",
    "markdown-pdf.quality": 100,
    "markdown-pdf.clip.x": 0,
    "markdown-pdf.clip.y": 0,
    "markdown-pdf.clip.width": 0,
    "markdown-pdf.clip.height": 0,
    "markdown-pdf.omitBackground": false,
    "markdown-pdf.executablePath": "",
    "markdown-pdf.scale": 1,
    "markdown-pdf.type": "pdf",
    "markdown-pdf.breaks": false,
    "markdown-pdf.highlight": true,
    "markdown-pdf.highlightStyle": "github",
    "markdown.extension.toc.levels": "1..6",
    "markdown.extension.toc.orderedList": false,
    "markdown.extension.toc.plaintext": false,
    "markdown.extension.toc.updateOnSave": false,
    "markdown.extension.toc.githubCompatibility": false,
    "markdown.extension.print.absoluteImgPath": true,
    "markdown.extension.print.imgToBase64": true,
    "markdown.extension.print.validateUrls": true
} 